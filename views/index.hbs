<html>

  <head>
    <meta charset='utf-8' />
    <title>Bwl | Home</title>
    <link rel='icon' type='image/x-icon' href='./assets/img/favicon.png' />
    <link
      rel='stylesheet'
      href='https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@48,400,0,0'
    />
  </head>

  <body>
    <div id='root'></div>
    {{#each posts}}
      <div
        class='app'
        messageId='{{this.messageId}}'
        user='{{this.totalComment}}'
        authorId='{{this.author.id}}'
        authorAvatar='{{this.author.avatar}}'
        authorName='{{this.author.username}}'
        totalLike='{{this.totalLike}}'
        createdTimestamp='{{this.createdTimestamp}}'
      >
        {{#each this.links}}
          <div class='app-link' link='{{this}}'>
          </div>
        {{/each}}
        {{#maxReaction this.reactions}}
          {{#each this}}
            <div
              class='app-react'
              reactId='{{this.id}}'
              reactName='{{this.name}}'
            ></div>
          {{/each}}
        {{/maxReaction}}
        {{#each this.reactions}}{{/each}}
      </div>
    {{/each}}
    <script src='https://unpkg.com/react/umd/react.production.min.js'></script>
    <script
      src='https://unpkg.com/react-dom/umd/react-dom.production.min.js'
    ></script>
    <script src='bundle.js'></script>
  </body>

</html>